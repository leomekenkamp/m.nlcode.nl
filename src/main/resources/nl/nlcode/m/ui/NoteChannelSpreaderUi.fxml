<fx:root 
    xmlns="http://javafx.com/javafx" 
    xmlns:fx="http://javafx.com/fxml/1"
    type="NoteChannelSpreaderUi"  
    styleClass="noteChannelSpreader"
>
    <instrument>
        <FieldVBox>
            <Field labelText="%inputChannel">
                <Spinner fx:id="inputChannel" editable="true">
                    <valueFactory>
                        <SpinnerValueFactory.IntegerSpinnerValueFactory min="0" max="15"/>
                    </valueFactory>
                </Spinner>
            </Field>
            <Field labelText="%useAsOutput">
                <GridPane styleClass="channels">
                    <CheckBox fx:id="useAsOutput0" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                    <CheckBox fx:id="useAsOutput1" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
                    <CheckBox fx:id="useAsOutput2" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
                    <CheckBox fx:id="useAsOutput3" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
                    <CheckBox fx:id="useAsOutput4" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
                    <CheckBox fx:id="useAsOutput5" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
                    <CheckBox fx:id="useAsOutput6" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
                    <CheckBox fx:id="useAsOutput7" GridPane.rowIndex="1" GridPane.columnIndex="3"/>
                    <CheckBox fx:id="useAsOutput8" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
                    <CheckBox fx:id="useAsOutput9" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
                    <CheckBox fx:id="useAsOutput10" GridPane.rowIndex="2" GridPane.columnIndex="2"/>
                    <CheckBox fx:id="useAsOutput11" GridPane.rowIndex="2" GridPane.columnIndex="3"/>
                    <CheckBox fx:id="useAsOutput12" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
                    <CheckBox fx:id="useAsOutput13" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
                    <CheckBox fx:id="useAsOutput14" GridPane.rowIndex="3" GridPane.columnIndex="2"/>
                    <CheckBox fx:id="useAsOutput15" GridPane.rowIndex="3" GridPane.columnIndex="3"/>
                </GridPane>
            </Field>
        </FieldVBox>
    </instrument>
</fx:root>
